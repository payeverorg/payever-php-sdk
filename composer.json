{
   "autoload" : {
      "classmap" : [
         "lib"
      ]
   },
   "autoload-dev" : {
      "classmap" : [
         "lib",
         "tests"
      ],
      "psr-4" : {
         "Payever\\Tests\\" : "tests/"
      }
   },
   "description" : "SDK PHP for payever payments",
   "homepage" : "https://github.com/payeverorg/payever-php-sdk",
   "keywords" : [
      "payments",
      "payever",
      "e-commerce"
   ],
   "license" : "MIT",
   "minimum-stability" : "stable",
   "name" : "payeverorg/payever-php-sdk",
   "require" : {
      "ext-curl" : "*",
      "ext-json" : "*",
      "php" : ">=5.6.0"
   },
   "require-dev" : {
      "friendsofphp/php-cs-fixer" : "^2.18",
      "phpmd/phpmd" : "^2.9",
      "phpunit/phpunit" : "^7.5",
      "squizlabs/php_codesniffer" : "^3.5"
   },
   "suggest" : {
      "ext-mcrypt" : "If you want to use mcrypt at StringHelper::generateRandom",
      "ext-openssl" : "If you want to use openssl at StringHelper::generateRandom",
      "ext-pdo" : "If you want to use MySQLLock"
   },
   "type" : "library",
   "version" : "0.0.3"
}
